{
    "manifest_version": 2,
    "name": "Tinder Auto Like",
    "description": "Extens√£o para automatizar o processo de like e match no Tinder",
    "version": "0.2.1",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
   	 "default_popup": "index.html"
    },
    "content_scripts": [{
        "matches": [
            "https://tinder.com/*"
        ],
        "css": [
            "style.css"
        ],
        "js": [
            "content.js"
        ]
    }],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "activeTab"
      ]
}