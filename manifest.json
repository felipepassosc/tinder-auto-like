{
    "manifest_version": 2,
    "name": "Tinder Like Automático",
    "description": "Extensão para automatizar o processo de like e match no Tinder",
    "version": "0.2.1",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [{
        "matches": [
            "https://tinder.com/app/recs",
            "https://tinder.com/app/matches"
        ],
        "css": [
            "style.css"
        ],
        "js": [
            "content.js"
        ]
    }]
}